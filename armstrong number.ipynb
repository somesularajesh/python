    "\n",
    "\n",
    "num = int(input(\"Enter any number : \"))\n",
    "temp = num\n",
    "sum=0\n",
    "while num > 0:\n",
    "    rem = num % 10\n",
    "    sum = rem*rem*rem+sum\n",
    "    num = num//10\n",
    "if sum==temp:\n",
    "    print(\"It is an Armstrong number\")\n",
    "else :\n",
    "    print(\"Not an Armstrong number\")"
